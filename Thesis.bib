journal = {Applied and Environmental Microbiology},
keywords = {16S,16S: genetics,Arctic Regions,Bacteria,Bacteria: classification,Bacteria: genetics,Bacteria: isolation \& purification,Bacteria: metabolism,Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial: chemistry,Bacterial: genetics,Biodiversity,Cluster Analysis,DNA,Flow Cytometry,Flow Cytometry: methods,Fluorescence,Fluorescence: methods,Microscopy,Molecular Sequence Data,Oceans and Seas,Organic Chemicals,Organic Chemicals: metabolism,Photosynthesis,Photosynthetic Reaction Center Complex Proteins,Photosynthetic Reaction Center Complex Proteins: g,Phylogeny,Polymerase Chain Reaction,Polymerase Chain Reaction: methods,RNA,Ribosomal,Ribosomal: chemistry,Ribosomal: genetics,Seasons,Seawater,Seawater: microbiology,Sequence Analysis},
month = aug,
