issn = {1462-2920},
journal = {Environmental Microbiology},
keywords = {16S,16S: genetics,Archaea,Archaea: classification,Archaea: isolation \& purification,Archaeal,Archaeal: chemistry,Archaeal: genetics,Arctic Regions,Bacteria,Bacteria: classification,Bacteria: isolation \& purification,Bacterial,Bacterial: chemistry,Bacterial: genetics,Biodiversity,Cluster Analysis,Cold Temperature,DNA,Eukaryota,Eukaryota: classification,Eukaryota: isolation \& purification,Genes,Ice Cover,Molecular Sequence Data,Nucleic Acid,Phylogeny,RNA,Ribosomal,Ribosomal: chemistry,Ribosomal: genetics,Seasons,Sequence Analysis,Sequence Homology,rRNA},
month = jul,
pages = {1828--1841},
pmid = {20192970},
title = {{Persistence of bacterial and archaeal communities in sea ice through an Arctic winter.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2916213\&tool=pmcentrez\&rendertype=abstract},
volume = {12},
year = {2010}
}
