journal = {Environmental Microbiology},
keywords = {16S,16S: genetics,Animals,Australia,Bacteria,Bacteria: classification,Bacteria: genetics,Bacterial,Bacterial: genetics,Camels,Camels: microbiology,DNA,Gastrointestinal Tract,Gastrointestinal Tract: microbiology,Gene Library,Metagenome,Molecular Sequence Data,Phylogeny,RNA,Ribosomal,Sequence Analysis},
