journal = {Nucleic Acids Research},
keywords = {Amino Acid,Animals,Cluster Analysis,Databases,Genomics,Internet,Phylogeny,Protein,Proteome,Proteome: classification,Proteome: genetics,Sequence Homology,User-Computer Interface},
month = jan,
pages = {D363--D368},
pmid = {16381887},
title = {{OrthoMCL-DB: querying a comprehensive multi-species collection of ortholog groups}},
