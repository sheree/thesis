journal = {Microbial Ecology},
keywords = {16S,16S: genetics,Antarctic Regions,Bacterial Physiological Phenomena,Colony Count,DNA Primers,Ecosystem,Electrophoresis,Fluorescence,Fresh Water,Fresh Water: microbiology,In Situ Hybridization,Microbial,Plankton,Plankton: physiology,Polyacrylamide Gel,Polymerase Chain Reaction,RNA,Ribosomal},
