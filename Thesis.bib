issn = {1462-2920},
journal = {Environmental Microbiology},
month = dec,
pmid = {23298151},
title = {{High-throughput isolation of giant viruses of the Mimiviridae and Marseilleviridae families in the Tunisian environment}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23298151},
year = {2012}
}
