keywords = {Bacterial Proteins,Bacterial Proteins: isolation \& purification,Chromatography,Liquid,Membrane Transport Proteins,Membrane Transport Proteins: isolation \& purificat,Oceans and Seas,Prochlorococcus,Prochlorococcus: chemistry,Proteome,Proteome: isolation \& purification,Seawater,Seawater: chemistry,Seawater: microbiology,Synechococcus,Synechococcus: chemistry,Tandem Mass Spectrometry},
month = jan,
pages = {93--105},
pmid = {18769456},
