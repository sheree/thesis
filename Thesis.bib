journal = {Analytical Chemistry},
keywords = {Algorithms,Databases,Factual,Mass Spectrometry,Models,Peptides,Peptides: analysis,Statistical},
month = oct,
pages = {5383--5392},
pmid = {12403597},
title = {{Empirical statistical model to estimate the accuracy of peptide identifications made by MS/MS and database search}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12403597},
volume = {74},
year = {2002}
}
@article{Denef2007,
abstract = {The recent surge in microbial genomic sequencing, combined with the development of high-throughput liquid chromatography-mass-spectrometry-based (LC/LC-MS/MS) proteomics, has raised the question of the extent to which genomic information of one strain or environmental sample can be used to profile proteomes of related strains or samples. Even with decreasing sequencing costs, it remains impractical to obtain genomic sequence for every strain or sample analyzed. Here, we evaluate how shotgun proteomics is affected by amino acid divergence between the sample and the genomic database using a probability-based model and a random mutation simulation model constrained by experimental data. To assess the effects of nonrandom distribution of mutations, we also evaluated identification levels using in silico peptide data from sequenced isolates with average amino acid identities (AAI) varying between 76 and 98\%. We compared the predictions to experimental protein identification levels for a sample that was evaluated using a database that included genomic information for the dominant organism and for a closely related variant (95\% AAI). The range of models set the boundaries at which half of the proteins in a proteomic experiment can be identified to be 77-92\% AAI between orthologs in the sample and database. Consistent with this prediction, experimental data indicated loss of half the identifiable proteins at 90\% AAI. Additional analysis indicated a 6.4\% reduction of the initial protein coverage per 1\% amino acid divergence and total identification loss at 86\% AAI. Consequently, shotgun proteomics is capable of cross-strain identifications but avoids most cross-species false positives.},
author = {Denef, Vincent J and Shah, Manesh B and Verberkmoes, Nathan C and Hettich, Robert L and Banfield, Jillian F},
doi = {10.1021/pr0701005},
file = {:home/sheree/phd/papers/papers-metaproteomics/Denef\_2007\_seq\_divergence\_proteomome.pdf:pdf},
issn = {1535-3893},
journal = {Journal of Proteome Research},
keywords = {Bacterial,Bacterial Proteins,Bacterial Proteins: analysis,Biofilms,Chromatography,Environmental Microbiology,Genetic,Genome,Liquid,Liquid: methods,Models,Mutation,Proteomics,Proteomics: methods,Species Specificity,Tandem Mass Spectrometry,Tandem Mass Spectrometry: methods},
month = aug,
pages = {3152--3161},
pmid = {17602579},
title = {{Implications of strain- and species-level sequence divergence for community and isolate shotgun proteomic analysis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17602579},
volume = {6},
year = {2007}
}
@article{Todd2010,
abstract = {A bacterium in the genus Halomonas that grew on dimethylsulfoniopropionate (DMSP) or acrylate as sole carbon sources and that liberated the climate-changing gas dimethyl sulfide in media containing DMSP was obtained from the phylloplane of the macroalga Ulva. We identified a cluster that contains genes specifically involved in DMSP catabolism (dddD, dddT) or in degrading acrylate (acuN, acuK) or that are required to break down both substrates (dddC, dddA). Using NMR and HPLC analyses to trace 13C- or 14C-labelled acrylate and DMSP in strains of Escherichia coli with various combinations of cloned ddd and/or acu genes, we deduced that DMSP is imported by the BCCT-type transporter DddT, then converted by DddD to 3-OH-propionate (3HP), liberating dimethyl sulfide in the process. As DddD is a predicted acyl CoA transferase, there may be an earlier, unidentified catabolite of DMSP. Acrylate is also converted to 3HP, via a CoA transferase (AcuN) and a hydratase (AcuK). The 3HP is predicted to be catabolized by an alcohol dehydrogenase, DddA, to malonate semialdehyde, thence by an aldehyde dehydrogenase, DddC, to acyl CoA plus CO2. The regulation of the ddd and acu genes is unusual, as a catabolite, 3HP, was a co-inducer of their transcription. This first description of genes involved in acrylate catabolism in any organism shows that the relationship between the catabolic pathways of acrylate and DMSP differs from that which had been suggested in other bacteria.},
author = {Todd, Jonathan D and Curson, Andrew RJ and Nikolaidou-Katsaraidou, Nefeli and Brearley, Charles A and Watmough, Nicholas J and Chan, Yohan and Page, Philip CB and Sun, Lei and Johnston, Andrew WB},
doi = {10.1111/j.1462-2920.2009.02071.x},
file = {:home/sheree/phd/papers/papers-DMS/Todd\_2010\_bacterial\_acrylate\_catabolism.pdf:pdf},
issn = {1462-2920},
journal = {Environmental Microbiology},
keywords = {Acrylates,Acrylates: metabolism,Bacterial,Coenzyme A-Transferases,Coenzyme A-Transferases: genetics,Coenzyme A-Transferases: metabolism,Genes,Halomonas,Halomonas: genetics,Halomonas: metabolism,Sulfides,Sulfides: metabolism,Sulfonium Compounds,Sulfonium Compounds: metabolism},
month = feb,
pages = {327--343},
pmid = {19807777},
title = {{Molecular dissection of bacterial acrylate catabolism - unexpected links with dimethylsulfoniopropionate catabolism and dimethyl sulfide production}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19807777},
volume = {12},
year = {2010}
}
