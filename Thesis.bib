issn = {1751-7370},
journal = {The ISME Journal},
keywords = {Antarctic Regions,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Chlorobi,Chlorobi: genetics,Chlorobi: isolation \& purification,Chlorobi: metabolism,Fresh Water,Fresh Water: microbiology,Molecular Sequence Data,Proteomics,Sulfur,Sulfur: metabolism,antarctic,cold adaptation,ecological dominance,green sulfur bacterium,meromictic lake,metagenomics,metaproteomics},
month = aug,
pages = {1002--19},
