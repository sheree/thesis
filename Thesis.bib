keywords = {Bacteria,Bacteria: metabolism,Bacteria: radiation effects,Heterotrophic Processes,Heterotrophic Processes: radiation effects,Light,Oceans and Seas,Seawater,Seawater: microbiology,Water Microbiology},
month = oct,
pages = {792--800},
