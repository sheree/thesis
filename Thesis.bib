journal = {FEMS Microbiology Ecology},
keywords = {16S,16S: genetics,Antarctic Regions,Bacterial,Bacterial: genetics,Ecosystem,Flavobacteriaceae,Flavobacteriaceae: classification,Flavobacteriaceae: genetics,Flavobacteriaceae: isolation \& purification,Geography,Oceans and Seas,Phylogeny,RNA,Ribosomal},
month = jan,
pages = {265--77},
pmid = {16329875},
title = {{Ecological and biogeographic relationships of class Flavobacteria in the Southern Ocean.}},
