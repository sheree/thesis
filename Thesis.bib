journal = {Applied and Environmental Microbiology},
keywords = {16S,16S: genetics,Antarctic Regions,Bacteria,Bacteria: genetics,Base Sequence,Biodiversity,Cluster Analysis,DNA,DNA Primers,DNA Primers: genetics,Gene Library,Ice Cover,Ice Cover: chemistry,Ice Cover: microbiology,Iron,Iron: analysis,Molecular Sequence Data,Phylogeny,RNA,Ribosomal,Sequence Analysis,Sulfates,Sulfates: analysis},
month = jun,
pages = {4029--4039},
pmid = {17468282},
title = {{Bacterial diversity associated with Blood Falls, a subglacial outflow from the Taylor Glacier, Antarctica.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1932727\&tool=pmcentrez\&rendertype=abstract},
